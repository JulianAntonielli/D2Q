<h1>Preguntas</h1>

<%= link_to 'New question', new_article_path %>


<table>
	<tr>
		<th>Pregunta</th>
		<th>Text</th>
	</tr>

	<% @articles.each do |article| %>
		<tr>
			<td><%= article.title %></td>
			<td><%= article.text %></td>
			<td><%= link_to 'Show', article_path(article) %></td>
		</tr>
	<% end %>
</table>